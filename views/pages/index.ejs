<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head') %>
  </head>
  <body>
    <header id="header" class="header fixed-top d-flex align-items-center">
      <div class="d-flex align-items-center justify-content-between">
        <a href="" class="logo d-flex align-items-center">
          <img src="img/logopln.png" alt="" />
          <span class="d-none d-lg-block">PORTAL ULPLTD KOTAMOBAGU</span>
        </a>
        <i class="bi bi-list toggle-sidebar-btn"></i>
      </div>
      <!-- End Logo -->

      <nav class="header-nav ms-auto">
        <ul class="d-flex align-items-center">
          <!-- End Search Icon-->

          <li class="nav-item dropdown pe-3">
            <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown">
              <img src="img/no-image.gif" alt="Profile" class="rounded-circle" />
              <span class="d-none d-md-block dropdown-toggle ps-2"><%= user.name %> </span> </a
            ><!-- End Profile Iamge Icon -->

            <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow profile">
              <li class="dropdown-header">
                <h6><%= user.name %></h6>
                <span><%= user.email %> </span>
              </li>
              <li>
                <hr class="dropdown-divider" />
              </li>

              <li>
                <hr class="dropdown-divider" />
              </li>

              <li>
                <a class="dropdown-item d-flex align-items-center" href="/logout">
                  <i class="bi bi-box-arrow-right"></i>
                  <span>Log Out</span>
                </a>
              </li>
            </ul>
            <!-- End Profile Dropdown Items -->
          </li>
          <!-- End Profile Nav -->
        </ul>
      </nav>
      <!-- End Icons Navigation -->
    </header>
    <!-- End Header -->

    <!-- Start Sidebar  -->
    <%- include('../partials/sidebar') %>
    <!-- End Sidebar  -->

    <!-- AWAL INPUT  -->
    <main id="main" class="main index-page">
      <!-- Mulai Carousel  -->
      <section class="section">
        <div class="row align-center justify-content-center">
          <div class="col-lg-11 mb-4">
            <!-- <div class="card align-center justify-content-center">
              <div class="card-body">
                <h5 class="card-title">With captions</h5> -->

            <!-- Slides with captions -->
            <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
              <div class="carousel-indicators">
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
              </div>
              <div class="carousel-inner">
                <div class="carousel-item active">
                  <img src="img/slides-1.jpeg" class="d-block w-100" alt="..." />
                  <div class="carousel-caption d-none d-md-block">
                    <!-- <h5 class="slide">Apel Siaga Nataru ULPLTD Kotamobagu</h5> -->
                    <p>Apel Siaga Nataru Tahun 2021</p>
                  </div>
                </div>
                <div class="carousel-item">
                  <img src="img/slides-2.jpeg" class="d-block w-100" alt="..." />
                  <div class="carousel-caption d-none d-md-block">
                    <!-- <h5 class="slide">Second slide label</h5> -->
                    <p>Arahan Manager Untuk Kesiapan Siaga Nataru</p>
                  </div>
                </div>
                <div class="carousel-item">
                  <img src="img/slides-3.jpeg" class="d-block w-100" alt="..." />
                  <div class="carousel-caption d-none d-md-block">
                    <!-- <h5 class="slide">Third slide label</h5> -->
                    <p>Penyerahan Snack Sebagai Simbolis</p>
                    <div class="filter">
                      <a class="icon" href="#" data-bs-toggle="dropdown"><i class="bi bi-three-dots"></i></a>
                      <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                        <li class="dropdown-header text-start">
                          <h6>Filter</h6>
                        </li>

                        <li><a class="dropdown-item" href="#">Today</a></li>
                        <li><a class="dropdown-item" href="#">This Month</a></li>
                        <li><a class="dropdown-item" href="#">This Year</a></li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>

              <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
              </button>
              <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
              </button>
            </div>
          </div>
        </div>
      </section>
      <!-- End Slides with captions -->
      <!-- Akhir Carousel  -->
      <section class="section dashboard">
        <div class="row">
          <!-- Left side columns -->
          <div class="col-lg-8">
            <div class="row">
              <!-- Sales Card -->
              <div class="col-xxl-4 col-xl-12">
                <div class="card info-card sales-card">
                  <div class="card-body">
                    <h5 class="card-title">Daya Terpasang</h5>

                    <div class="d-flex align-items-center">
                      <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                        <i class="bi-lightning-charge"></i>
                      </div>
                      <div class="ps-3">
                        <h6>8.000 kW</h6>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!-- End Sales Card -->

              <!-- Revenue Card -->
              <div class="col-xxl-4 col-xl-12">
                <div class="card info-card revenue-card">
                  <div class="card-body">
                    <h5 class="card-title">Daya Mampu</h5>

                    <div class="d-flex align-items-center">
                      <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                        <i class="bi-lightning-charge"></i>
                      </div>
                      <div class="ps-3">
                        <h6>6.000 kW</h6>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!-- End Revenue Card -->

              <!-- Customers Card -->
              <div class="col-xxl-4 col-xl-12">
                <div class="card info-card customers-card">
                  <div class="card-body">
                    <h5 class="card-title">Jumlah Mesin</h5>

                    <div class="d-flex align-items-center">
                      <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                        <i class="bi-gear"></i>
                      </div>
                      <div class="ps-3">
                        <h6>6 Unit</h6>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!-- End Customers Card -->

              <!-- Reports -->
              <div class="col-12">
                <div class="card">
                  <div class="card-body">
                    <h5 class="card-title">Kinerja <span>/Unit</span></h5>
                    <div class="col-12 d-flex justify-content-lg-center align-content-center">
                      <div class="btn-group" role="group" aria-label="Basic mixed styles example">
                        <ul class="nav nav-pills" id="pills-tab" role="tablist">
                          <li class="nav-item" role="presentation">
                            <button class="nav-link active" data-bs-toggle="pill" data-bs-target="#triidx" type="button">
                              <i class="bi-circle-fill" style="font-size: 15px" data-bs-toggle="tooltip" data-bs-placement="top" title="EAF, SOF, EFOR"></i>
                            </button>
                          </li>
                          <li class="nav-item" role="presentation">
                            <button class="nav-link" data-bs-toggle="pill" data-bs-target="#psidx" type="button"><i class="bi-circle-fill" style="font-size: 15px" data-bs-toggle="tooltip" data-bs-placement="top" title="PS"></i></button>
                          </li>
                          <li class="nav-item" role="presentation">
                            <button class="nav-link" data-bs-toggle="pill" data-bs-target="#sfcidx" type="button"><i class="bi-circle-fill" style="font-size: 15px" data-bs-toggle="tooltip" data-bs-placement="top" title="SFC"></i></button>
                          </li>
                        </ul>
                      </div>
                    </div>

                    <div class="tab-content pt-2" id="myTabContent">
                      <div class="tab-pane fade show active" id="triidx" role="tabpanel" aria-labelledby="home-tab">
                        <!-- Line Chart -->
                        <canvas id="chartEAFU" style="max-height: 400px"></canvas>
                        <script>
                          const eafu = <%- EAFU %>;
                          const sofu = <%- SOFU %>;
                          const eforu = <%- EFORU %>;
                        </script>
                        <!-- End Line CHart -->
                      </div>

                      <div class="tab-pane fade" id="psidx" role="tabpanel" aria-labelledby="profile-tab">
                        <!-- Line Chart -->
                        <canvas id="chartPSU" style="max-height: 400px"></canvas>
                        <script>
                          const psu = <%- PSU %>;
                        </script>
                        <!-- End Line CHart -->
                      </div>

                      <div class="tab-pane fade" id="sfcidx" role="tabpanel" aria-labelledby="profile-tab">
                        <!-- Line Chart -->
                        <canvas id="chartSFCU" style="max-height: 400px"></canvas>
                        <script>
                          const sfcu = <%- SFCU %>;
                        </script>
                        <!-- End Line CHart -->
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!-- End Reports -->

              <!-- Lokasi ULPLTD Kotamobagu  -->
              <div class="col-12">
                <div class="card">
                  <div class="card-body">
                    <h5 class="card-title">Lokasi <span>ULPLTD Kotamobagu</span></h5>
                    <div id="googleMap" style="width: 100%; height: 380px"></div>
                    <a href="https://www.google.com/maps/place/Kotamobagu+Diesel+Power+Station/@0.7464965,124.3188817,17z/data=!3m1!4b1!4m5!3m4!1s0x327e3d6a17b86187:0x5064c4eb8df24bc4!8m2!3d0.74648!4d124.3210703"
                      ><p style="font-style: italic; text-align: center" class="mt-2">Jl. Arif Rahman Hakim, Biga, Kotamobagu Utara, Kota Kotamobagu, Sulawesi Utara</p></a
                    >
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!-- End Left side columns -->

          <!-- Right side columns -->
          <div class="col-lg-4">
            <!-- Tombol Navigasi Data  -->
            <div class="card d-flex justify-content-between">
              <div class="card-body">
                <h5 class="card-title">Jumlah Pegawai</h5>
                <div class="row d-flex text-center fw-bold justify-content-evenly mb-2">
                  <div class="col-sm-3 card-icon">
                    <i class="bi-people-fill"></i>
                    <h6 class="mb-3">PLN</h6>
                    <p>5</p>
                  </div>
                  <div class="col-sm-3 card-icon">
                    <i class="bi-people-fill"></i>
                    <h6 class="mb-3">O&M</h6>
                    <p>20</p>
                  </div>
                  <div class="col-sm-3 card-icon">
                    <i class="bi-people-fill"></i>
                    <h6 class="mb-3">CS</h6>
                    <p>3</p>
                  </div>
                  <div class="col-sm-3 card-icon">
                    <i class="bi-people-fill"></i>
                    <h6 class="mb-3">Security</h6>
                    <p>6</p>
                  </div>
                </div>
              </div>
            </div>
            <!-- End Tombol Navigasi Data  -->

            <!-- Website Traffic -->
            <div class="card">
              <div class="card-body pb-0">
                <h5 class="card-title">Produksi <span>| 2021</span></h5>
                <!-- <canvas id="chartProd" style="max-height: 400px; justify-content: center"></canvas> -->
                <script>
                  const prod = <%- Prod %>;
                </script>
                <table class="table table-striped" style="text-align: center; font-size: smaller; font-weight: bold">
                  <tbody>
                    <% produksi.forEach((prod, i) => { %>
                    <tr>
                      <td>Unit <%= i+1 %></td>
                      <td><%= prod.toLocaleString() %> kWh</td>
                    </tr>
                    <% }) %>
                  </tbody>
                </table>
              </div>
            </div>

            <!-- Bahan Bakar -->
            <div class="card">
              <div class="card-body">
                <div class="filter">
                  <a class="icon" href="#" data-bs-toggle="dropdown"><i class="bi bi-three-dots"></i></a>
                  <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                    <li><a class="dropdown-item" href="#">Update Data</a></li>
                  </ul>
                </div>
                <h5 class="card-title">Ketersediaan <span>Bahan Bakar</span></h5>
                <div class="row">
                  <div class="col-12">
                    <img src="img/oil-tank2.png" style="opacity: calc(0.4)" width="650" height="auto" class="img-fluid rounded-start" alt="" />
                    <div class="progress mt-3">
                      <div class="progress-bar bg-warning progress-bar-striped progress-bar-animated" role="progressbar" style="width: 75.5%" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100">75 Liter</div>
                    </div>
                    <div class="row mt-4 align-content-center justify-content-center">
                      <div class="col-7">
                        <h3 style="font-weight: bolder">HOP 14 Hari</h3>
                      </div>
                      <div class="col-5">
                        <img src="img/good.png" width="25%" data-bs-toggle="tooltip" data-bs-placement="top" title="AMAN" alt="" />
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- End Website Traffic -->
          </div>
          <!-- End Right side columns -->
        </div>
      </section>
    </main>
    <!-- End #main -->
    <!-- AKHIR INPUT  -->

    <footer><%- include('../partials/footer') %></footer>
  </body>
</html>
