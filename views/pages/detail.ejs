<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head') %>
  </head>
  <body>
    <header id="header" class="header fixed-top d-flex align-items-center">
      <div class="d-flex align-items-center justify-content-between">
        <a href="" class="logo d-flex align-items-center">
          <img src="public/img/logopln2.png" alt="" />
          <!-- <span class="d-none d-lg-block">PORTAL ULPLTD KOTAMOBAGU</span> -->
        </a>
        <i class="bi bi-list toggle-sidebar-btn"></i>
        <span class="d-none d-lg-block ms-3 fs-3 fw-bolder">PORTAL ULPLTD KOTAMOBAGU</span>
      </div>
      <!-- End Logo -->

      <nav class="header-nav ms-auto">
        <ul class="d-flex align-items-center">
          <li class="nav-item dropdown pe-3">
            <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown">
              <img src="public/img/no-image.gif" alt="Profile" class="rounded-circle" />
              <span class="d-none d-md-block dropdown-toggle ps-2"><%= user.name %> </span> </a
            ><!-- End Profile Iamge Icon -->

            <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow profile">
              <li class="dropdown-header">
                <h6><%= user.name %></h6>
                <span><%= user.email %> </span>
              </li>
              <li>
                <hr class="dropdown-divider" />
              </li>

              <li>
                <hr class="dropdown-divider" />
              </li>

              <li>
                <a class="dropdown-item d-flex align-items-center" href="/logout">
                  <i class="bi bi-box-arrow-right"></i>
                  <span>Log Out</span>
                </a>
              </li>
            </ul>
            <!-- End Profile Dropdown Items -->
          </li>
          <!-- End Profile Nav -->
        </ul>
      </nav>
      <!-- End Icons Navigation -->
    </header>
    <!-- End Header -->

    <!-- Start Sidebar  -->
    <%- include('../partials/sidebar') %>
    <!-- End Sidebar  -->

    <!-- AWAL INPUT  -->
    <main id="main" class="main">
      <!-- <div class="pagetitle">
      <h1>Form Input Data</h1>
      </div>End Page Title -->
      <section class="section detail-kinerja">
        <div class="row">
          <form class="row mb-3 justify-content-end" action="/detail" method="GET" enctype="multipart/form-data">
            <div class="col-md-4">
              <select id="selectmonth" class="form-select d-inline" name="bulanData">
                <option selected>Pilih Bulan</option>
                <option>1</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                <option>5</option>
                <option>6</option>
                <option>7</option>
                <option>8</option>
                <option>9</option>
                <option>10</option>
                <option>11</option>
                <option>12</option>
              </select>
            </div>
            <div class="col-md-4">
              <select id="selectyear" class="form-select" name="tahunData">
                <option selected>Pilih Tahun</option>
                <option>2020</option>
                <option>2021</option>
              </select>
            </div>
            <div class="col-md-4">
              <div class="text-center d-inline">
                <button type="submit" class="btn btn-light"><i class="ri-send-plane-fill"></i></button>
              </div>
            </div>
          </form>
          <!-- <div class="col-12 mb-3 d-flex justify-content-end align-content-center">
            
          </div> -->
          <div class="col-lg-12">
            <div class="card">
              <div class="card-body">
                <h5 class="card-title">Data Kinerja</h5>
                <table class="table table-detail">
                  <thead>
                    <tr>
                      <th scope="col">KPI</th>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <th scope="col">Unit <%= i+1 %></th>
                      <% } %>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td>EAF (%)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].eaf).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>EFOR (%)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].efor).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>SOF (%)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].sof).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>CF (%)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].cf).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>PS (%)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%= Math.round(detail[i].ps).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>SFC (Liter/kWh)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].sfcBruto).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>NPHR (kCal/kWh)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].taraKalorNettoPLTD).toLocaleString()%></td>
                      <% } %>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>

            <div class="card">
              <div class="card-body">
                <h5 class="card-title">Data Pengusahaan</h5>
                <table class="table table-detail">
                  <thead>
                    <tr>
                      <th scope="col">Parameter</th>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <th scope="col">Unit <%= i+1 %></th>
                      <% } %>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td>Daya Terpasang (kW)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%= detail[i].dayaTerpasang.toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Daya Mampu (kW)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%= detail[i].dayaMampu.toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Produksi (kWh)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].produksi).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>PS Sentral (kWh)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].psSentral).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>PS Trafo (kWh)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%= Math.round(detail[i].psTrafo).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Pemakaian BBM (Liter)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].pemakaianBBM).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Plan Outage (Jam)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].po).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Maintenance Outage (Jam)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].mo).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Force Outage (Jam)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].fo).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Service Hour (Jam)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%= Math.round(detail[i].sh).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Reserve Service Hour (Jam)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%= Math.round(detail[i].rsh).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Trip Internal (Kali)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%= detail[i].tripin.toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>Trip Eksternal (Kali)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%= detail[i].tripex.toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>EPDH (Jam)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].epdh).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>EUDH (Jam)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].eudh).toLocaleString()%></td>
                      <% } %>
                    </tr>
                    <tr>
                      <td>ESDH (Jam)</td>
                      <% for( let i = 0; i < 6; i++ ) { %>
                      <td><%=Math.round(detail[i].esdh).toLocaleString()%></td>
                      <% } %>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </section>
    </main>
    <!-- End #main -->
    <!-- AKHIR INPUT  -->
    <footer><%- include('../partials/footer') %></footer>
  </body>
</html>
